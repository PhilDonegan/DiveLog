<div class="row">
	<form method="post" enctype="multipart/form-data" asp-controller="Upload" asp-action="Upload" id="form1">
		<div class="form-group">
			<div class="col-md-10">
				<input type="file" name="file" />
			</div>
		</div>
		<div class="form-group">
			<div class="col-md-5">
				<p>1/3 File Upload</p>
				<div class="progress">
					<div id="upload-progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:0%;">
						0%
					</div>
				</div>
				<p>2/3 Process Dives</p>
				<div class="progress">
					<div id="process-progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:0%;">
						0%
					</div>
				</div>
				<p>3/3 Store Dive Data</p>
				<div class="progress">
					<div id="store-progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:0%;">
						0%
					</div>
				</div>
			</div>
		</div>
		<div class="form-group">
			<div class="col-md-10">
				<input type="submit" value="Upload" />
				<label id="responseText"></label>
			</div>
		</div>
	</form>
</div>
